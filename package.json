    "@nuxtjs/style-resources": "^1.0.0",
    "@types/leaflet": "^1.5.16",
    "@types/luxon": "^1.25.0",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "gh-pages": "^2.2.0",
    "jest": "^26.5.3",
    "prettier": "^2.1.2",
    "stylus": "^0.54.8",
    "stylus-loader": "^3.0.2",
    "stylus-supremacy": "^2.14.5",
    "ts-jest": "^26.4.1",
